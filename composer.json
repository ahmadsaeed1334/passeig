{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "anlutro/l4-settings": "^1.3",
        "benjamincrozat/laravel-dropbox-driver": "^4.0",
        "cmgmyr/messenger": "^2.29",
        "filament/filament": "^3.2",
        "guzzlehttp/guzzle": "^7.2",
        "jantinnerezo/livewire-alert": "^3.0",
        "joedixon/laravel-translation": "^2.2",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^10.10",
        "laravel/jetstream": "^4.2",
        "laravel/sanctum": "^3.3",
        "laravel/slack-notification-channel": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.4",
        "laravolt/avatar": "^5.0",
        "league/iso3166": "^4.3",
        "livewire/livewire": "^3.4",
        "nicolaslopezj/searchable": "^1.13",
        "outhebox/blade-flags": "^1.2",
        "rap2hpoutre/laravel-log-viewer": "^2.3",
        "rinvex/laravel-categories": "^7.1",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-backup": "^8.6",
        "spatie/laravel-permission": "^6.3",
        "stevebauman/location": "^7.1",
        "tamer-dev/laravel-env-cli": "^1.2",
        "tinymce/tinymce": "^7.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.10",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "m1ge0/bootsjet": "^1.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
